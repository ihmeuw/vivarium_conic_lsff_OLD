components:
    vivarium_public_health:
        population:
            - BasePopulation()
            - Mortality()
            - FertilityCrudeBirthRate()
        disease.models:
            - SIS('diarrheal_diseases')
            - SIS('lower_respiratory_infections')
            - SIR_fixed_duration('measles', '10')  # days
            - NeonatalSWC_without_incidence('neonatal_sepsis_and_other_neonatal_infections')
            - NeonatalSWC_without_incidence('neonatal_encephalopathy_due_to_birth_asphyxia_and_trauma')
            - NeonatalSWC_without_incidence('hemolytic_disease_and_other_neonatal_jaundice')
        metrics:
            - MortalityObserver()
            - DisabilityObserver()

#    vivarium_conic_lsff.components:

configuration:
    input_data:
        location: {{ location_proper }}
        input_draw_number: 0
        artifact_path: /share/costeffectiveness/artifacts/vivarium_conic_lsff/{{ location_sanitized }}.hdf
    interpolation:
        order: 0
        extrapolate: False
    randomness:
        map_size: 1_000_000
        key_columns: ['entrance_time']
        random_seed: 0
    time:
        start:
            year: 2005
            month: 7
            day: 2
        end:
            year: 2010
            month: 7
            day: 2
        step_size: 1 # Days
    population:
        population_size: 10000
        age_start: 0
        age_end: 5

    metrics:
        disability:
            by_age: True
            by_sex: True
            by_year: True
        mortality:
            by_age: True
            by_sex: True
            by_year: True
